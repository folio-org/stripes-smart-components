    "@folio/stripes-util": "^5.1.0",
    "@formatjs/cli": "^4.2.20",
    "axe-core": "4.4.3",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "eslint": "^7.5.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "faker": "^4.1.0",
    "inflected": "^2.0.4",
